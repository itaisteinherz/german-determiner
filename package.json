	},
	"devDependencies": {
		"@babel/plugin-transform-object-assign": "^7.2.0",
		"babel-eslint": "^10.0.1",
		"eslint": "^5.16.0",
		"eslint-config-xo-react": "^0.19.0",
		"eslint-plugin-react": "^7.13.0",
		"eslint-plugin-react-hooks": "^1.6.0",
